{
  "name": "vektor-dashboard",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "biome lint --write",
    "format": "biome format --write",
    "check": "biome check --write",
    "start": "react-router-serve build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "e2e:test": "exec playwright test",
    "e2e:test:ui": "exec playwright test --ui",
    "e2e:test:chrome": "exec playwright test --project=chromium",
    "e2e:test:firefox": "exec playwright test --project=firefox",
    "e2e:test:webkit": "exec playwright test --project=webkit",
    "e2e:test:generate": "exec playwright codegen",
    "e2e:show-report": "exec playwright show-report",
    "e2e:install": "exec playwright install"
  },
  "dependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@clerk/remix": "^4.2.39",
    "@iconify-icon/react": "^2.1.0",
    "@mantine/core": "^7.13.0",
    "@mantine/hooks": "^7.13.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@react-router/node": "7.0.0-pre.1",
    "@react-router/serve": "7.0.0-pre.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "7.0.0-pre.1",
    "react-router-dom": "7.0.0-pre.1",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.0",
    "@playwright/test": "^1.48.1",
    "@react-router/dev": "7.0.0-pre.1",
    "@types/node": "^22.7.7",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.45",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.1.6",
    "vite": "^5.4.4",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "engines": {
    "node": "20.x",
    "pnpm": "9.x"
  }
}
